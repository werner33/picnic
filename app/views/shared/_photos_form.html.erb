

<%= form_for(@photo_gallery, :html => { :multipart => true }) do |f| %>
 
  <%= f.fields_for :photos do |p| %>

     <%= p.label :image %><br>
     <%= p.file_field :image, :multiple => true, name: "photos[image][]" %>
 
  <% end %>

 <%= f.submit "Upload photos", class: "subscribe-button" %>
<% end %>
